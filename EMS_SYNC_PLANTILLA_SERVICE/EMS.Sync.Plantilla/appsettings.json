  /* 
    ======================== UNIT OF TIME ============================
    0 = Get all records,
    1 = Get all newly created or recently modified records for the past [n] seconds,
    2 = Get all newly created or recently modified records for the past [n] minutes,
    3 = Get all newly created or recently modified records for the past [n] hours,
    4 = Get all newly created or recently modified records for the past [n] days
    [n] = is "Get Modified by Value"

    Ex. 
    * Position_GetModifiedBy_UnitOfTime = 2
    * Position_GetModifiedBy_Value = 10
   
     this will Get all Created or Modified Position records for the past 10 minutes 

  */

  "Position_IntervalInSeconds": "120", /*every 2 minutes*/
  "Position_GetModifiedBy_UnitOfTime": "2",
  "Position_GetModifiedBy_Value": "5",
  "Position_GetModifiedBy_UnitOfTime_OnStartUp": "4",
  "Position_GetModifiedBy_Value_OnStartUp": "30",

  "PositionLevel_IntervalInSeconds": "86400", /*everyday*/
  "PositionLevel_GetModifiedBy_UnitOfTime": "4",
  "PositionLevel_GetModifiedBy_Value": "2",
  "PositionLevel_GetModifiedBy_UnitOfTime_OnStartUp": "4",
  "PositionLevel_GetModifiedBy_Value_OnStartUp": "30",

  "OrgGroup_IntervalInSeconds": "300", /*every 5 minutes*/
  "OrgGroup_GetModifiedBy_UnitOfTime": "0",
  "OrgGroup_GetModifiedBy_Value": "10",
  "OrgGroup_GetModifiedBy_UnitOfTime_OnStartUp": "4",
  "OrgGroup_GetModifiedBy_Value_OnStartUp": "30",

  "OrgGroupPosition_IntervalInSeconds": "60", /*every 1 minutes*/
  "OrgGroupPosition_GetModifiedBy_UnitOfTime": "0",
  "OrgGroupPosition_GetModifiedBy_Value": "3",
  "OrgGroupPosition_GetModifiedBy_UnitOfTime_OnStartUp": "4",
  "OrgGroupPosition_GetModifiedBy_Value_OnStartUp": "30",

  "Employee_IntervalInSeconds": "120", /*every 2 minutes*/
  "Employee_GetModifiedBy_UnitOfTime": "0",
  "Employee_GetModifiedBy_Value": "5",
  "Employee_GetModifiedBy_UnitOfTime_OnStartUp": "4",
  "Employee_GetModifiedBy_Value_OnStartUp": "30",

  "EmployeeRoving_IntervalInSeconds": "120", /*every 2 minutes*/
  "EmployeeRoving_GetModifiedBy_UnitOfTime": "0",
  "EmployeeRoving_GetModifiedBy_Value": "5",
  "EmployeeRoving_GetModifiedBy_UnitOfTime_OnStartUp": "4",
  "EmployeeRoving_GetModifiedBy_Value_OnStartUp": "30",

  "EmployeeMovement_IntervalInSeconds": "120", /*every 2 minutes*/
  "EmployeeMovement_GetModifiedBy_UnitOfTime": "0",
  "EmployeeMovement_GetModifiedBy_Value": "5",
  "EmployeeMovement_GetModifiedBy_UnitOfTime_OnStartUp": "4",
  "EmployeeMovement_GetModifiedBy_Value_OnStartUp": "30",

  "PSGCRegion_IntervalInSeconds": "120", /*every 2 minutes*/
  "PSGCRegion_GetModifiedBy_UnitOfTime": "2",
  "PSGCRegion_GetModifiedBy_Value": "5",
  "PSGCRegion_GetModifiedBy_UnitOfTime_OnStartUp": "4",
  "PSGCRegion_GetModifiedBy_Value_OnStartUp": "30",

  "PSGCCity_IntervalInSeconds": "120", /*every 2 minutes*/
