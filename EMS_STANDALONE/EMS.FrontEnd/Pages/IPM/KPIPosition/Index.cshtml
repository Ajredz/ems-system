@page
@model EMS.FrontEnd.Pages.IPM.KPIPosition.IndexModel
@{
    ViewData["Title"] = "KPI Position";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

@* KPIPosition Modal *@
<div id="divKPIPositionModal" class="modal fade" data-backdrop="static" data-keyboard="false" tabindex="-1" role="dialog" style="width:90%">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div id="divKPIPositionBodyModal"></div>
        </div>
    </div>
</div>
@* End of KPIPosition Modal *@

@* Copy KPIPosition Modal *@
<div id="divCopyKPIPositionModal" class="modal fade" data-backdrop="static" data-keyboard="false" tabindex="-1" role="dialog" style="width:30%">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div id="divCopyKPIPositionBodyModal"></div>
        </div>
    </div>
</div>
@* End of Copy KPIPosition Modal *@

@await Html.PartialAsync("~/Pages/IPM/KPIPosition/_List.cshtml", Model._systemURL)
