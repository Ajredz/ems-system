@model List<EMS_SecurityServiceModel.SystemURL.SystemURL>

<label class="list-title"> My KPI Score List </label>

<div id="filterFieldsContainer" style="display:none">
    <div class="filterFields"><input type="search" placeholder="Search.." id="txtFilterRecordID" maxlength="8" class="form-control" /></div>
    <div class="filterFields">
        <input type="search" placeholder="Search.." id="txtFilterID" maxlength="255" class="form-control" />
        <div id="multiSelectedID" class="form-control multiselect-control"></div>
    </div>
    <div class="filterFields"><input type="search" placeholder="Search.." id="txtFilterDescription" maxlength="500" class="form-control" /></div>
    <div class="filterFields">
        <input type="search" placeholder="From" id="txtFilterDateFromFrom" maxlength="10" class="form-control" />
        <input type="search" placeholder="To" id="txtFilterDateFromTo" maxlength="10" class="form-control" />
    </div>
    <div class="filterFields">
        <input type="search" placeholder="From" id="txtFilterDateToFrom" maxlength="10" class="form-control" />
        <input type="search" placeholder="To" id="txtFilterDateToTo" maxlength="10" class="form-control" />
    </div>
    <div class="filterFields">
        <input type="search" placeholder="Min" id="txtFilterScoreFrom" maxlength="10" class="form-control text-amount" />
        <input type="search" placeholder="Max" id="txtFilterScoreTo" maxlength="10" class="form-control text-amount" />
    </div>
    <div class="filterFields">
        <input type="search" placeholder="Search.." id="txtFilterOrgGroup" maxlength="255" class="form-control" />
        <div id="multiSelectedOrgGroup" class="form-control multiselect-control"></div>
    </div>
    <div class="filterFields">
        <input type="search" placeholder="Search.." id="txtFilterPosition" maxlength="255" class="form-control" />
        <div id="multiSelectedPosition" class="form-control multiselect-control"></div>
    </div>
@*    <div class="filterFields"></div>
    <div class="filterFields"></div>
    <div class="filterFields">
        <div class="dropdown">
            <div id="multiSelectedSourceType" class="form-control multiselect-control" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
            </div>
            <div id="multiSelectedSourceTypeOption" class="dropdown-menu form-control multiselect-itemlist">
            </div>
        </div>
    </div>*@
</div>

<div class="ViewList no-margin-top">
    <div id="divMyKPIScoreListErrorMessage"></div>
    <div class="ViewButtons">
        <span class="list-icon glyphicon glyphicon-refresh" title="Reset List" id="btnReset"></span>
        <span class="list-icon glyphicon glyphicon-search" title="Search List" id="btnSearch"></span>
    </div>
    <table id="tblIPMMyKPIScoreList"></table>
    <div id="divPager"></div>
</div>

