<div class="form-horizontal">
    <div class="form-group form-fields text-align-left">
        @if (ViewData["HasEditDescriptionFeature"] + "" == "true")
        {
            <button type="button" id="btnEditDescription" class="btnBlue formbtn">Save</button>
            <button type="button" id="btnRerun" class="btnBlue formbtn">Rerun</button>
        }

        <span class="rightAlign">
            <label class="control-label block-label">Run Time :</label>
            <label class="control-label block-label" id="lblRuntime"></label>
        </span>
    </div>

</div>
<div id="divEmployeeScoreDescriptionErrorMessage" class="div-error-message"></div>
<div class="form-horizontal">
    <form method="post" onsubmit="return false" id="frmRerunEmployeeScore">
        <div class="white-block mt-10">
            <table class="table summaryTable">
                <tr>
                    <td class="col-label">
                        <label class="control-label block-label">Run ID :</label>
                    </td>
                    <td>
                        <label class="control-label block-label" id="lblRunID"></label>
                    </td>
                    <td class="col-label">
                        <label class="control-label block-label">Run Date :</label>
                    </td>
                    <td>
                        <label class="control-label block-label" id="lblCreatedDate"></label>
                    </td>
                </tr>

                <tr>
                    <td class="col-label">
                        <label class="control-label block-label">Report Name :</label>
                    </td>
                    <td>
                        @if (ViewData["HasEditDescriptionFeature"] + "" == "true")
                        {
                            <textarea rows="3" cols="10" wrap="hard" id="txtDescription" class="form-control required-field" maxlength="500" title="Description" placeholder="Period and Org Group"></textarea>
                            <input type="hidden" id="hdnRunID" />
                        }
                        else
                        {
                            <label class="control-label block-label" id="lblDescription"></label>
                        }
                    </td>
                    <td class="col-label">
                        <label class="control-label block-label">Is Active ? :</label>
                    </td>
                    <td>
                        @if (ViewData["HasEditDescriptionFeature"] + "" == "true")
                        {
                            <input type="checkbox" id="cbIsTransActive" title="Is Active?">
                        }
                        else
                        {
                            <label class="control-label block-label" id="lblIsTransActive"></label>
                        }
                    </td>
                </tr>

            </table>
        </div>
        <div class="white-block mt-20">
            <div class="col-label block-title">
                <label class="control-label">Criteria</label>
            </div>
            <table class="table summaryTable">
                <tr>
                    <td class="col-label">
                        <label class="control-label block-label">Date From :</label>
                    </td>
                    <td>
                        <label class="control-label block-label">
                            <span id="lblRunDateFrom"></span>
                        </label>
                    </td>
                    <td class="col-label">
                        <label class="control-label block-label">Date To :</label>
                    </td>
                    <td>
                        <label class="control-label block-label">
                            <span id="lblRunDateTo"></span>
                        </label>
                    </td>
                </tr>
                <tr>
                    <td class="col-label">
                        <label class="control-label block-label">Filter by :</label>
                    </td>
                    <td>
                        <label class="control-label block-label" id="lblFilterBy"></label>
                    </td>
                    <td class="col-label">
                        <label class="control-label block-label">Use Recent KPI Position :</label>
                    </td>
                    <td>
                        <label class="control-label block-label" id="lblUseRecent"></label>
                    </td>
                </tr>
                <tr>
                    <td class="col-label">
                        <label class="control-label block-label">Org Group :</label>
                    </td>
                    <td>
                        <label class="control-label block-label" id="lblFilterOrgGroup"></label>
                    </td>
                    <td class="col-label">
                        <label class="control-label block-label">Override Existing :</label>
                    </td>
                    <td>
                        <label class="control-label block-label" id="lblOverride"></label>
                    </td>
                </tr>
                <tr>
                    <td class="col-label">
                        <label class="control-label block-label">Position :</label>
                    </td>
                    <td>
                        <label class="control-label block-label" id="lblFilterPosition"></label>
                    </td>
                    <td class="col-label">
                        <label class="control-label block-label">Included Levels Below :</label>
                    </td>
                    <td>
                        <label class="control-label block-label" id="lblLevelsBelow"></label>
                    </td>
                </tr>
                <tr>
                    <td class="col-label">
                        <label class="control-label block-label">Employee :</label>
                    </td>
                    <td>
                        <label class="control-label block-label" id="lblFilterEmployee"></label>
                    </td>
                </tr>
            </table>
        </div>
        <div class="white-block mt-20">
            <div class="col-label block-title">
                <label class="control-label">Summary</label>
            </div>
            <table class="table summaryTable">
                @*<tr>
                        <td class="col-label">
                        </td>
                        <td>
                        </td>
                        <td class="col-label">
                            <label class="control-label block-label">Ratings</label>
                        </td>
                        <td>
                            <label class="control-label block-label">Count</label>
                        </td>
                    </tr>*@
                <tr>
                    <td class="col-label">
                        <label class="control-label block-label" title="Exceeds Expectation">EE (<span id="lblRatingEEMin"></span>%-<span id="lblRatingEEMax"></span>%) :</label>
                    </td>
                    <td>
                        <label class="control-label block-label font-weight-bold" id="lblRatingEEEmployees" title="Exceeds Expectation"></label>
                    </td>
                    <td class="col-label">
                        <label class="control-label block-label">Total Active Employees :</label>
                    </td>
                    <td>
                        <label class="control-label block-label font-weight-bold" id="lblTotalEmployees"></label>
                    </td>
                </tr>
                <tr>
                    <td class="col-label">
                        <label class="control-label block-label" title="Meets Expectation">ME (<span id="lblRatingMEMin"></span>%-<span id="lblRatingMEMax"></span>%) :</label>
                    </td>
                    <td>
                        <label class="control-label block-label font-weight-bold" id="lblRatingMEEmployees" title="Meets Expectation"></label>
                    </td>
                    <td class="col-label">
                        @*<label class="control-label block-label">Employees with IPM :</label>*@
                        <label class="control-label block-label">IPM - Qualified Employees :</label>
                    </td>
                    <td>
                        <label class="control-label block-label font-weight-bold" id="lblEmployeesWithIPM"></label>
                    </td>
                </tr>
                <tr>
                    <td class="col-label">
                        <label class="control-label block-label" title="Slightly Below Expectation">SBE (<span id="lblRatingSBEMin"></span>%-<span id="lblRatingSBEMax"></span>%) :</label>
                    </td>
                    <td>
                        <label class="control-label block-label font-weight-bold" id="lblRatingSBEEmployees" title="Slightly Below Expectation"></label>
                    </td>
                    <td class="col-label">
                        <label class="control-label block-label">Total Employees w/ IPM :</label>
                    </td>
                    <td>
                        <label class="control-label block-label font-weight-bold" id="lblTotalEmployeesWithIPM"></label>
                    </td>
                </tr>
                <tr>
                    <td class="col-label">
                        <label class="control-label block-label" title="Below Expectation">BE (<span id="lblRatingBEMin"></span>%-<span id="lblRatingBEMax"></span>%) :</label>
                    </td>
                    <td>
                        <label class="control-label block-label font-weight-bold" id="lblRatingBEEmployees" title="Below Expectation"></label>
                    </td>
                    <td class="col-label">
                        <label class="control-label block-label">Employees w/ Multiple Movement :</label>
                    </td>
                    <td>
                        <label class="control-label block-label font-weight-bold" id="lblEmployeesWithMultiple"></label>
                    </td>
                </tr>
                <tr>
                    <td class="col-label">
                    </td>
                    <td>
                    </td>
                    <td class="col-label">
                        <label class="control-label block-label">Total IPM Result :</label>
                    </td>
                    <td>
                        <label class="control-label block-label font-weight-bold" id="lblTotalIPMResult"></label>
                    </td>
                </tr>
            </table>
        </div>
    </form>
    @if (ViewData["HasEditDescriptionFeature"] + "" == "true")
    {
        <div id="divTransProgress" hidden>
            <div class="form-group form-fields no-margin-bottom">
                <div class="col-md-12 col-label">
                    <div class="progress">
                        <div class="progress-bar" role="progressbar" aria-valuenow="0"
                             aria-valuemin="0" aria-valuemax="100" style="width:0%" id="progressbar">
                            <span class="sr-only"></span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="form-group form-fields no-margin-top">
                <div class="col-md-12 col-label text-align-center">
                    <label class="control-label block-label" id="lblProgressDetails">
                    </label>
                </div>
            </div>

            <div id="divOnGoingOverlay">
            </div>
        </div>
    }
</div>