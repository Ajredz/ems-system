
<div class="modal-header">
    <span id="txtFunction"></span> Kickout Question
    <button type="button" class="close" data-dismiss="modal">&times;</button>
</div>
<div class="modal-body">
    <div class="form-horizontal">
        <div class="form-group form-fields">
            <form method="post" onsubmit="return false" id="formKickoutQuestion">
                <div class="form-group form-fields text-align-left">
                    <button type="button" id="btnSaveKickoutQuestion" class="btnBlue formbtn" hidden>Save</button>
                    <button type="button" id="btnEditKickoutQuestion" class="btnBlue formbtn" hidden>Edit</button>
                    <button type="button" id="btnUpdateKickoutQuestion" class="btnBlue formbtn" hidden>Update</button>
                    <button type="button" id="btnBackKickoutQuestion" class="btnBlue formbtn" hidden>Back</button>
                </div>
                <div class="form-group form-fields text-align-left">
                    <div class="col-md-12 text-align-left list-header">
                    </div>
                </div>

                <div id="divKickoutQuestionErrorMessage" class="div-error-message"></div>

                <input type="hidden" name="ID" id="hdnMRFKickoutQuestionID" />
                <input type="hidden" name="MRFID" id="hdnKickoutQuestionMRFID" />

                <div class="row">
                    <div class="white-block">
                        <div class="col-label block-title">
                            <label class="control-label">Details</label>
                        </div>

                        <div class="form-group form-fields">
                            <div class="col-md-4 col-label">
                                <label class="control-label block-label" for="txtKickoutQuestionCode"> <span class="reqField">* </span>Question Code</label>
                            </div>
                            <div class="col-md-7">
                                <input name="KickoutQuestionID" type="hidden" id="hdnKickoutQuestionID" />
                                <input type="text" id="txtKickoutQuestionCode" class="form-control required-field" title="Question" maxlength="100" disabled />
                            </div>
                        </div>

                        <div class="form-group form-fields">
                            <div class="col-md-4 col-label">
                                <label class="control-label block-label" for="txtType">Type</label>
                            </div>
                            <div class="col-md-7">
                                <input id="txtKickoutQuestionType" class="form-control" title="Type" disabled />
                            </div>
                        </div>

                        <div class="form-group form-fields">
                            <div class="col-md-4 col-label">
                                <label class="control-label block-label" for="txtQuestion">Question</label>
                            </div>
                            <div class="col-md-7">
                                <input id="txtKickoutQuestionQuestion" class="form-control" title="Question" disabled />
                            </div>
                        </div>

                        <div class="form-group form-fields">
                            <div class="col-md-4 col-label">
                                <label class="control-label block-label" for="txtAnswer">Answer</label>
                            </div>
                            <div class="col-md-7">
                                <input id="txtKickoutQuestionAnswer" class="form-control" title="Answer" disabled />
                            </div>
                        </div>

                        <div class="form-group form-fields">
                            <div class="col-md-4 col-label">
                                <label class="control-label block-label" for="txtOrder"> <span class="reqField">* </span>Order</label>
                            </div>
                            <div class="col-md-7">
                                <input name="Order" id="txtKickoutQuestionOrder" class="form-control required-field" title="Order" maxlength="100" disabled />
                            </div>
                        </div>


                    </div>

                </div>
            </form>
        </div>
    </div>
</div>