@model List<EMS_SecurityServiceModel.SystemURL.SystemURL>

<label class="list-title"> MRF Approval List </label>

<div id="filterFieldsContainer" style="display:none">
    @*<div class="filterFields"><input type="search" placeholder="Search.." id="txtFilterMRFID" maxlength="70" class="form-control" /></div>*@
    <div class="filterFields"><input type="search" placeholder="Search.." id="txtFilterMRFTransactionID" maxlength="70" class="form-control" /></div>
    <div class="filterFields">
        <input type="search" placeholder="Search.." id="txtFilterOrgGroup" maxlength="255" class="form-control" />
        <div id="multiSelectedOrgGroup" class="form-control multiselect-control"></div>
    </div>
    <div class="filterFields">
        <input type="search" placeholder="Search.." id="txtFilterPositionLevel" maxlength="255" class="form-control" />
        <div id="multiSelectedPositionLevel" class="form-control multiselect-control"></div>
    </div>
    <div class="filterFields">
        <input type="search" placeholder="Search.." id="txtFilterPosition" maxlength="255" class="form-control" />
        <div id="multiSelectedPosition" class="form-control multiselect-control"></div>
    </div>
    <div class="filterFields">
        <div class="dropdown">
            <div id="multiSelectedNatureOfEmployment" class="form-control multiselect-control" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
            </div>
            <div id="multiSelectedNatureOfEmploymentOption" class="dropdown-menu form-control multiselect-itemlist">
            </div>
        </div>
    </div>
    <div class="filterFields">
        <input type="search" placeholder="Min" id="txtFilterNoOfApplicantMin" maxlength="10" class="form-control" />
        <input type="search" placeholder="Max" id="txtFilterNoOfApplicantMax" maxlength="10" class="form-control" />
    </div>
    <div class="filterFields">
        <div class="dropdown">
            <div id="multiSelectedStatus" class="form-control multiselect-control" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
            </div>
            <div id="multiSelectedStatusOption" class="dropdown-menu form-control multiselect-itemlist">
            </div>
        </div>
    </div>
    <div class="filterFields">
        <input type="search" placeholder="From" id="txtFilterDateCreatedFrom" maxlength="10" class="form-control" />
        <input type="search" placeholder="To" id="txtFilterDateCreatedTo" maxlength="10" class="form-control" />
    </div>
    <div class="filterFields">
        <input type="search" placeholder="From" id="txtFilterDateApprovedFrom" maxlength="10" class="form-control" />
        <input type="search" placeholder="To" id="txtFilterDateApprovedTo" maxlength="10" class="form-control" />
    </div>
    <div class="filterFields">
        <input type="search" placeholder="Min" id="txtFilterAgeMin" maxlength="10" class="form-control" />
        <input type="search" placeholder="Max" id="txtFilterAgeMax" maxlength="10" class="form-control" />
    </div>
</div>

<div class="ViewList">
    <div class="ViewButtons" id="divMRFApprovalFilter">
        <span class="list-icon glyphicon glyphicon-refresh" title="Reset List" id="btnReset"></span>
        <span class="list-icon glyphicon glyphicon-search" title="Search List" id="btnSearch"></span>
    </div>
    <table id="tblManpowerMRFApprovalList"></table>
    <div id="divPager"></div>
</div>

@*<script src="~/js/Pages/_EMSCommon.min.js"></script>*@
<script src="~/js/Pages/Manpower/MRFApproval/Lists.js"></script>