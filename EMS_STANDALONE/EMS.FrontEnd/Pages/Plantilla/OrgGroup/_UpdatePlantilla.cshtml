@model List<EMS.Plantilla.Transfer.OrgGroup.PlantillaCountUpdateForm>

    <div class="modal-header">
        Update Plantilla Count
        <button type="button" class="close" data-dismiss="modal">&times;</button>
    </div>

    <div class="modal-body">
        <form method="post" onsubmit="return false" id="frmOrgGroupPlantillaCountUpdate">
            <div class="form-horizontal">
                <div class="form-group form-fields">
                    <div class="col-md-4 text-align-left">
                        <button type="button" id="btnSave" class="btnBlue formbtn">Save</button>
                    </div>
                </div>
                <div class="form-group form-fields">
                    <div class="col-md-8 text-align-left list-header">
                        <label id="lblOrgGroupDescription"></label>
                    </div>
                </div>
                <div class="white-block">

                    <div class="tab">
                        <button class="tablinks" onclick="return objEMSCommonJS.ChangeTab(this,'divUpdatePlantillaModal #tabOrgGroupPosition')">
                            <span class="control-label">Manpower</span>
                        </button>
                        <button class="tablinks" onclick="return objEMSCommonJS.ChangeTab(this,'divUpdatePlantillaModal #tabOrgGroupNPRF')">
                            <span class="control-label">NPRF</span>
                        </button>
                    </div>

                    <partial name="~/Pages/Plantilla/OrgGroup/_NPRF.cshtml" model="new List<EMS.Plantilla.Transfer.OrgGroup.OrgGroupNPRFForm>()" />

                    <div class="tabcontent" id="tabOrgGroupPosition">
                        <div class="form-group form-fields">
                            <div class="col-md-1 dynamic-field-header" style="width:50px;">
                                <label class="control-label block-label">Head</label>
                            </div>
                            <div class="col-md-3 dynamic-field-header">
                                <label class="control-label block-label">Position</label>
                            </div>
                            <div class="col-md-3 dynamic-field-header">
                                <label class="control-label block-label">Reporting Position</label>
                            </div>
                            <div class="col-md-0-75 dynamic-field-header">
                                <label class="control-label block-label">Planned</label>
                            </div>
                            <div class="col-md-0-75 dynamic-field-header">
                                <label class="control-label block-label">Active</label>
                            </div>
                            <div class="col-md-0-75 dynamic-field-header">
                                <label class="control-label block-label">Inactive</label>
                            </div>
                            <div class="col-md-0-75 dynamic-field-header">
                                <label class="control-label block-label">Variance</label>
                            </div>
                        </div>

                        @{int ctr = 0;}

                        @if (Model != null)
                        {
                            @foreach (var item in Model)
                            {
                    <div class="form-group form-fields">
                        @*<div class="col-md-0-5">
                        </div>*@
                        <input type="hidden" asp-for="@Model[ctr].OrgGroupID" name="PlantillaCount[@ctr].OrgGroupID" id="hdnUpdateOrgGroupID_@ctr" />
                        <input type="hidden" asp-for="@Model[ctr].PositionID" name="PlantillaCount[@ctr].PositionID" id="hdnUpdatePositionID_@ctr" />
                        <input type="hidden" asp-for="@Model[ctr].ReportingPositionID" name="PlantillaCount[@ctr].ReportingPositionID" id="hdnUpdateReportingPositionID_@ctr" />
                        <div class="col-md-1 no-padding text-align-center" style="width:50px;">
                            <input asp-for="@Model[ctr].IsHead" name="PlantillaCount[@ctr].IsHead" type="checkbox" class="IsHeadDynamicFields" title="Head" readonly>
                        </div>
                        <div class="col-md-3">
                            <label class="control-label block-label">@Model[ctr].PositionDescription</label>
                        </div>
                        <div class="col-md-3">
                            <label class="control-label block-label">@Model[ctr].ReportingPositionDescription</label>
                        </div>
                        <div class="col-md-0-75 text-align-right no-padding">
                            @if ((ViewData["HasUpdatePlannedFeature"] + "") == "true")
                            {
                                <input asp-for="@Model[ctr].PlannedCount" name="PlantillaCount[@ctr].PlannedCount" id="txtUpdatePlannedCount_@ctr" type="text" class="form-control text-amount UpdatePlannedDynamicFields" title="Planned">
                            }
                            else
                            {
                                <input asp-for="@Model[ctr].PlannedCount" name="PlantillaCount[@ctr].PlannedCount" id="txtUpdatePlannedCount_@ctr" type="text" class="form-control text-amount UpdatePlannedDynamicFields" title="Planned" readonly>
                            }

                        </div>
                        <div class="col-md-0-75 text-align-right no-padding">
                            @if ((ViewData["HasUpdateActiveFeature"] + "") == "true")
                            {
                                <input asp-for="@Model[ctr].ActiveCount" name="PlantillaCount[@ctr].ActiveCount" id="txtUpdateActiveCount_@ctr" type="text" class="form-control text-amount UpdateActiveDynamicFields" title="Active" readonly>
                            }
                            else
                            {
                                <input asp-for="@Model[ctr].ActiveCount" name="PlantillaCount[@ctr].ActiveCount" id="txtUpdateActiveCount_@ctr" type="text" class="form-control text-amount UpdateActiveDynamicFields" title="Active" readonly>
                            }
                        </div>
                        <div class="col-md-0-75 text-align-right no-padding">
                            @if ((ViewData["HasUpdateInactiveFeature"] + "") == "true")
                            {
                                <input asp-for="@Model[ctr].InactiveCount" name="PlantillaCount[@ctr].InactiveCount" id="txtUpdateInactiveCount_@ctr" type="text" class="form-control text-amount UpdateInactiveDynamicFields" title="Inactive" readonly>
                            }
                            else
                            {
                                <input asp-for="@Model[ctr].InactiveCount" name="PlantillaCount[@ctr].InactiveCount" id="txtUpdateInactiveCount_@ctr" type="text" class="form-control text-amount UpdateInactiveDynamicFields" title="Inactive" readonly>
                            }
                        </div>
                        <div class="col-md-0-75 text-align-right no-padding">
                            <label class="control-label block-label remarks-label text-amount total-label UpdateVarianceDynamicFields" id="lblUpdateVarianceCount_@ctr">@(Model[ctr].ActiveCount - Model[ctr].PlannedCount)</label>
                        </div>
                    </div>
                                ctr++;

                            }
                        }
                    <div class="form-group form-fields text-align-right">
                        <div class="col-md-1" style="width:50px;">
                        </div>
                        <div class="col-md-3">
                        </div>
                        <div class="col-md-3">
                            <label class="control-label block-label total-label">Total :</label>
                        </div>

                        <div class="col-md-0-75">
                            <label id="txtUpdateTotalPlanned" class="control-label block-label total-label UpdateTotalFields"></label>
                        </div>
                        <div class="col-md-0-75">
                            <label id="txtUpdateTotalActive" class="control-label block-label total-label UpdateTotalFields"></label>
                        </div>
                        <div class="col-md-0-75">
                            <label id="txtUpdateTotalInactive" class="control-label block-label total-label UpdateTotalFields"></label>
                        </div>
                        <div class="col-md-0-75">
                            <label id="txtUpdateTotalVariance" class="control-label block-label total-label UpdateTotalFields"></label>
                        </div>
                    </div>
                    </div>
                </div>
            </div>
        </form>
    </div>