@model List<EMS_SecurityServiceModel.SystemURL.SystemURL>

<label class="list-title"> Quick Employee Update </label>

<div id="filterFieldsContainer" style="display:none">
    <div class="filterFields"><input type="search" placeholder="Search.." id="txtFilterEmployeeID" maxlength="70" class="form-control" /></div>
    <div class="filterFields"><input type="search" placeholder="Search.." id="txtFilterCode" maxlength="70" class="form-control" /></div>
    <div class="filterFields"><input type="search" placeholder="Search.." id="txtFilterName" maxlength="70" class="form-control" /></div>
    <div class="filterFields"><input type="search" placeholder="Search.." id="txtFilterCorporateEmail" maxlength="70" class="form-control" /></div>
    <div class="filterFields"><input type="search" placeholder="Search.." id="txtFilterOfficeMobile" maxlength="70" class="form-control" /></div>
    <div class="filterFields">
        <input type="search" placeholder="Search.." id="txtFilterOrgGroup" maxlength="255" class="form-control" />
        <div id="multiSelectedOrgGroup" class="form-control multiselect-control"></div>
    </div>
    <div class="filterFields">
        <input type="search" placeholder="Search.." id="txtFilterPosition" maxlength="255" class="form-control" />
        <div id="multiSelectedPosition" class="form-control multiselect-control"></div>
    </div>
    <div class="filterFields">
        <div class="dropdown">
            <div id="multiSelectedEmploymentStatus" class="form-control multiselect-control" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
            </div>
            <div id="multiSelectedEmploymentStatusOption" class="dropdown-menu form-control multiselect-itemlist">
            </div>
        </div>
    </div>
    <div class="filterFields"><input type="search" placeholder="Search.." id="txtFilterOldEmployeeID" maxlength="70" class="form-control" /></div>
</div>

<div class="ViewList">
    <div class="ViewButtons" id="divSystemUserFilter">
        <span class="list-icon glyphicon glyphicon-export" title="Export List" id="btnExport"></span>
        <span class="list-icon glyphicon glyphicon-refresh" title="Reset List" id="btnReset"></span>
        <span class="list-icon glyphicon glyphicon-search" title="Search List" id="btnSearch"></span>
        @*<button type="button" id="btnExport" class="list-button btnBlue formbtn">Export</button>*@
    </div>
    <table id="tblAdministratorCorporateEmailList"></table>
    <div id="divPager"></div>
</div>

<script src="~/js/Pages/Administrator/EmployeeCorporateEmail/Lists.js"></script>