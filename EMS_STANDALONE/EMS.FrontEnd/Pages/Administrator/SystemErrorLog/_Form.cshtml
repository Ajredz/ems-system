@model EMS.Security.Transfer.SystemErrorLog.Form

<div class="modal-header">
    Error Log
    <button type="button" class="close" data-dismiss="modal">&times;</button>
</div>
<div class="modal-body">
    <div class="form-horizontal">
        <div class="form-group form-fields">
            <form method="post" onsubmit="return false" id="frmSystemErrorLog">
                <div class="form-horizontal">
                    <div class="form-group form-fields text-align-right">
                        <div class="col-md-12">
                        </div>
                    </div>

                    <div id="divSystemUserErrorMessage" class="div-error-message"></div>
                    <input type="hidden" asp-for="@Model.ID" name="ErrorLog.ID" id="hdnID" />

                    <div class="white-block">
                        <div class="col-label block-title">
                            <label class="control-label">Details</label>
                        </div>
                        <div class="form-group form-fields">
                            <div class="col-md-2 col-label">
                                <label class="control-label block-label"> <span class="unreqField">* </span>Layer </label>
                            </div>
                            <div class="col-md-10">
                                <label class="control-label block-label"> <span class="unreqField">* </span>@Model.Layer </label>
                            </div>
                        </div>
                        <div class="form-group form-fields">
                            <div class="col-md-2 col-label">
                                <label class="control-label block-label"> <span class="unreqField">* </span>Class </label>
                            </div>
                            <div class="col-md-10">
                                <label class="control-label block-label"> <span class="unreqField">* </span>@Model.Class </label>
                            </div>
                        </div>
                        <div class="form-group form-fields">
                            <div class="col-md-2 col-label">
                                <label class="control-label block-label"> <span class="unreqField">* </span>Method </label>
                            </div>
                            <div class="col-md-10">
                                <label class="control-label block-label"> <span class="unreqField">* </span>@Model.Method </label>
                            </div>
                        </div>
                        <div class="form-group form-fields">
                            <div class="col-md-2 col-label">
                                <label class="control-label block-label"> <span class="unreqField">* </span>Error Message </label>
                            </div>
                            <div class="col-md-10">
                                <label class="control-label block-label"> <span class="unreqField">* </span>@Model.ErrorMessage </label>
                            </div>
                        </div>
                        <div class="form-group form-fields">
                            <div class="col-md-2 col-label">
                                <label class="control-label block-label"> <span class="unreqField">* </span>Inner Exception </label>
                            </div>
                            <div class="col-md-10">
                                <label class="control-label block-label"> <span class="unreqField">* </span>@Model.InnerException </label>
                            </div>
                        </div>
                        <div class="form-group form-fields">
                            <div class="col-md-2 col-label">
                                <label class="control-label block-label"> <span class="unreqField">* </span>User </label>
                            </div>
                            <div class="col-md-10">
                                <label class="control-label block-label"> <span class="unreqField">* </span>@Model.User </label>
                            </div>
                        </div>
                        <div class="form-group form-fields">
                            <div class="col-md-2 col-label">
                                <label class="control-label block-label"> <span class="unreqField">* </span>CreatedDate </label>
                            </div>
                            <div class="col-md-10">
                                <label class="control-label block-label"> <span class="unreqField">* </span>@Model.CreatedDate </label>
                            </div>
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>
<div class="modal-footer">
</div>


