@model List<EMS_SecurityServiceModel.SystemURL.SystemURL>

<label class="list-title"> Quick Branch Update </label>

<div id="filterFieldsContainer" style="display:none">
    <div class="filterFields"><input type="search" placeholder="Search.." id="txtFilterOrgGroupID" maxlength="8" class="form-control" /></div>
    <div class="filterFields"><input type="search" placeholder="Search.." id="txtFilterCode" maxlength="50" class="form-control" /></div>
    <div class="filterFields"><input type="search" placeholder="Search.." id="txtFilterDescription" maxlength="255" class="form-control" /></div>
    <div class="filterFields"><input type="search" placeholder="Search.." id="txtFilterCategory" maxlength="255" class="form-control" /></div>
    <div class="filterFields"><input type="search" placeholder="Search.." id="txtFilterEmail" maxlength="255" class="form-control" /></div>
    <div class="filterFields"><input type="search" placeholder="Search.." id="txtFilterNumber" maxlength="255" class="form-control" /></div>
    <div class="filterFields"><input type="search" placeholder="Search.." id="txtFilterAddress" maxlength="255" class="form-control" /></div>
    <div class="filterFields">
        <div class="dropdown">
            <div id="multiSelectedOrgType" class="form-control multiselect-control" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
            </div>
            <div id="multiSelectedOrgTypeOption" class="dropdown-menu form-control multiselect-itemlist">
            </div>
        </div>
    </div>
    <div class="filterFields"><input type="search" placeholder="Search.." id="txtFilterParentOrg" maxlength="255" class="form-control" /></div>
    <div class="filterFields">
        <div class="dropdown">
            <div id="multiSelectedIsBranchActive" class="form-control multiselect-control" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
            </div>
            <div id="multiSelectedIsBranchActiveOption" class="dropdown-menu form-control multiselect-itemlist">
            </div>
        </div>
    </div>
    <div class="filterFields">
        <input type="search" placeholder="Min" id="txtServiceBayCountMin" maxlength="10" class="form-control" />
        <input type="search" placeholder="Max" id="txtServiceBayCountMax" maxlength="10" class="form-control" />
    </div>
</div>

<div class="ViewList">
    <div class="ViewButtons" id="divSystemUserFilter">
        <span class="list-icon glyphicon glyphicon-export" title="Export List" id="btnExport"></span>
        <span class="list-icon glyphicon glyphicon-refresh" title="Reset List" id="btnReset"></span>
        <span class="list-icon glyphicon glyphicon-search" title="Search List" id="btnSearch"></span>
        @*<button type="button" id="btnExport" class="list-button btnBlue formbtn">Export</button>*@
    </div>
    <table id="tblAdministratorBranchInfoList"></table>
    <div id="divPager"></div>
</div>

<script src="~/js/Pages/Administrator/BranchInfo/Lists.js"></script>