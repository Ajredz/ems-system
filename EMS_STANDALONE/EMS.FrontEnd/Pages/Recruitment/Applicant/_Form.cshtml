                            <div class="col-md-3">
                                <input asp-for="@Model.DateApplied" type="text" name="Applicant.DateApplied" id="dpDateApplied" class="form-control required-field" title="Date Applied" readonly />
                            </div>
                            <div class="col-md-2-5 col-label">
                                <label class="control-label block-label" for="txtExpectedSalary"> <span class="unreqField">* </span>Expected Salary</label>
