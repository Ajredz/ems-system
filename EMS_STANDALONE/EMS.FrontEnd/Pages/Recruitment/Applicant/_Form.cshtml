                </div>
                <div class="col-md-3">
                    <select asp-for="@Model.WorkflowID" asp-items="@((List<SelectListItem>)ViewData["WorkflowSelectList"])" name="Applicant.WorkflowID" id="ddlWorkflow" class="form-control required-field" title="Recruitment Workflow" readonly>
