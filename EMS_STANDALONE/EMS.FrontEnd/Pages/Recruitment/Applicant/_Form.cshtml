@model EMS.Recruitment.Transfer.Applicant.Form

<div class="modal-header">
    Applicant
    <button type="button" class="close" data-dismiss="modal">&times;</button>
</div>
<div class="modal-body modal-body-fixed-height">
    <div class="form-horizontal">
        <div class="form-group form-fields">
            <form method="post" onsubmit="return false" id="frmApplicant">
                <div class="form-horizontal">
                    
                    <div class="form-group form-fields">
                        <div class="col-md-4 text-align-left">
                            @if (ViewData["HasDeleteFeature"] + "" == "true")
                            {
                                @Html.Raw("<button type=\"button\" id=\"btnDelete\" class=\"btnBlue formbtn\">Delete</button>")
                            }
                            @if (ViewData["HasEditFeature"] + "" == "true")
                            {
                                @Html.Raw("&nbsp;<button type=\"button\" id=\"btnEdit\" class=\"btnBlue formbtn\">Edit</button>")
                            }
                            <button type="button" style="display:none" id="btnSave" class="btnBlue formbtn">Save</button>
                            <button type="button" style="display:none" id="btnBack" class="btnBlue formbtn">Back</button>
                            <button type="button" id="btnApplicantChangeStatus" class="btnBlue formbtn button-width-auto" hidden>Change Status</button>
                        </div>
                    </div>
                    <div class="form-group form-fields">
                        <div class="col-md-8 text-align-left list-header">
                            @if (Model.PersonalInformation != null)
                            {
                                <label>@(Model.PersonalInformation.LastName + ", " + Model.PersonalInformation.FirstName + " " + Model.PersonalInformation.MiddleName)</label>
                            }
                            @if (Model.PositionRemarks != null)
                            {
                                <label>| @(Model.PositionRemarks)</label>
                            }
                        </div>
                    </div>
                    <div id="divApplicantErrorMessage" class="div-error-message"></div>
                    <input type="hidden" asp-for="@Model.ID" name="Applicant.ID" id="hdnID" />
                    <input type="hidden" asp-for="@Model.MRFTransactionID" name="Applicant.MRFTransactionID" id="hdnMRFTransactionID" />
                    <input type="hidden" id="hdnWorkflowCode" value="RECRUITMENT" />
                    @*<input type="hidden" asp-for="@Model.CurrentStepCode" name="Applicant.CurrentStepCode"/>
        <input type="hidden" asp-for="@Model.CurrentStepDescription" name="Applicant.CurrentStepDescription"/>
        <input type="hidden" asp-for="@Model.CurrentStepApproverRoleIDs" name="Applicant.CurrentStepApproverRoleIDs"/>*@

                    <div class="white-block">
                        <div class="col-label block-title">
                            <label class="control-label">Details</label>
                        </div>
