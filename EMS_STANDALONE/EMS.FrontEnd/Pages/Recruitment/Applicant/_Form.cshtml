                    <div class="white-block">
                        <div class="tab">
                            <button class="tablinks" onclick="return objEMSCommonJS.ChangeTab(this,'tabPersonalInformation', '#divApplicantModal')">
                                <span class="control-label">Personal Information</span>
                            </button>
                            <button class="tablinks" onclick="return objEMSCommonJS.ChangeTab(this,'tabAttachment', '#divApplicantModal')">
                                <span class="control-label">Attachment</span>
                            </button>
                            <button class="tablinks" onclick="return objEMSCommonJS.ChangeTab(this,'tabApplicationHistory', '#divApplicantModal')">
                                <span class="control-label">Application History (MRF)</span>
                            </button>
                            <button class="tablinks" onclick="return objEMSCommonJS.ChangeTab(this,'tabLogActivity', '#divApplicantModal')">
                                <span class="control-label">Task Checklist</span>
                            </button>
                            <button class="tablinks" id="tabIdLegalProfile" onclick="return objEMSCommonJS.ChangeTab(this,'tabLegalProfile', '#divApplicantModal')">
                                <span class="control-label">Legal Profile</span>
                            </button>
                        </div>

                        <partial name="~/Pages/Recruitment/Applicant/_PersonalInformation.cshtml" model="Model.PersonalInformation" />
                        <partial name="~/Pages/Recruitment/Applicant/_Attachment.cshtml" model="Model.Attachments" />
                        <partial name="~/Pages/Recruitment/Applicant/_ApplicationHistory.cshtml" model="Model.ApplicationHistory" />
                        @*<partial name="~/Pages/Recruitment/Applicant/_LogActivity.cshtml" />*@
                        <div class="tabcontent" id="tabLogActivity"></div>
                        <div class="tabcontent" id="tabLegalProfile"></div>

                    </div>

                </div>
            </form>
        </div>
    </div>
</div>
<div class="modal-footer">
</div>