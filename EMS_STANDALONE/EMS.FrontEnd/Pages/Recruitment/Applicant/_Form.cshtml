                    <select asp-for="@Model.PositionID" asp-items="@((List<SelectListItem>)ViewData["PositionSelectList"])" name="Applicant.PositionID" id="ddlDesiredPosition" class="form-control required-field" title="Desired Position" readonly>
                        <option value="">- Select an item -</option>
                    </select>
                </div>*@
                            <div class="col-md-2 col-label">
                                <label class="control-label block-label" for="txtPositionRemarks"> <span class="reqField">* </span>Desired Position/s</label>
                            </div>
                            <div class="col-md-3">
                                <input asp-for="@Model.PositionRemarks" type="text" name="Applicant.PositionRemarks" id="txtPositionRemarks" maxlength="100" class="form-control required-field" title="Desired Position (remarks)" readonly />
                            </div>
                            <div class="col-md-2-5 col-label">
                                <label class="control-label block-label" for="txtReferredByUserIDDescription"> <span class="unreqField">* </span>Referred By</label>
                            </div>
                            <div class="col-md-3">
                                <input asp-for="@Model.ReferredByUserID" type="hidden" name="Applicant.ReferredByUserID" id="hdnReferredByUserID" class="form-control" readonly />
                                <input asp-for="@Model.ReferredByUserIDDescription" type="search" name="Applicant.ReferredByUserIDDescription" placeholder="Search.." id="txtReferredByUserIDDescription" maxlength="255" class="form-control" title="Referred By" readonly />
                            </div>
                        </div>

