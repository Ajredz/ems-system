                            <div class="col-md-3">
                                <select asp-for="@Model.ApplicationSource" asp-items="@((List<SelectListItem>)ViewData["ApplicationSourceSelectList"])" name="Applicant.ApplicationSource" id="ddlApplicationSource" class="form-control required-field" title="Application Source" readonly>
