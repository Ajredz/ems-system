@page
@model EMS.FrontEnd.Pages.Recruitment.ApplicantTagging.IndexModel
@{
  ViewData["Title"] = "Tag Applicant to Position";
  Layout = "~/Pages/Shared/_Layout.cshtml";
}

@await Html.PartialAsync("~/Pages/Recruitment/ApplicantTagging/_List.cshtml", Model._systemURL)

@* Applicant Tagging Task Modal *@
<div id="divApplicantTaggingModal" class="modal fade" data-backdrop="static" data-keyboard="false" tabindex="-1" role="dialog" style="width:80%">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            @*<div id="divApplicantTaggingHeaderModal" class="modal-header"></div>*@
            <div id="divApplicantTaggingBodyModal" class="modal-body"></div>
            @*<div id="divApplicantTaggingFooterModal" class="modal-footer"></div>*@
        </div>
    </div>
</div>
@* End of Applicant Tagging Task Modal *@

@* Applicant Modal *@
<div id="divApplicantModal" class="modal fade" data-backdrop="static" data-keyboard="false" tabindex="-1" role="dialog" style="width:90%">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div id="divApplicantBodyModal"></div>
        </div>
    </div>
</div>
