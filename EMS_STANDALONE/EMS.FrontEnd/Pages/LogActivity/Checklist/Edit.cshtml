@page
@model EMS.FrontEnd.Pages.LogActivity.Checklist.EditModel

@if ((ViewData["ApplicantName"] + "") != "")
{
    @await Html.PartialAsync("~/Pages/LogActivity/CommonForms/_ApplicantUpdateForm.cshtml", Model.ApplicantLogActivityForm)

    <script src="~/js/Pages/LogActivity/Checklist/UpdateApplicantLogActivity.js"></script>
}


@if ((ViewData["EmployeeName"] + "") != "")
{
    @await Html.PartialAsync("~/Pages/LogActivity/CommonForms/_EmployeeUpdateForm.cshtml", Model.EmployeeLogActivityForm)

    <script src="~/js/Pages/LogActivity/Checklist/UpdateEmployeeLogActivity.js"></script>
}
