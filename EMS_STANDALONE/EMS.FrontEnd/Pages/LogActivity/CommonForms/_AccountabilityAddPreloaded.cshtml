<div class="modal-header">
    <span id="spanAccountability">Add Preloaded Accountability</span>
    <button type="button" class="close" data-dismiss="modal">&times;</button>
</div>
<div class="modal-body">
    <div class="form-horizontal">
        <div class="form-group form-fields">
            <form method="post" onsubmit="return false" id="frmPreAccountability">
                <div class="form-horizontal">
                    <div class="form-group form-fields text-align-left">
                        <div class="col-md-12">
                            <button type="button" id="btnSavePreAccountability" class="btnBlue formbtn">Save</button>
                        </div>
                    </div>

                    <div id="divAccountabilityAddPreloadedErrorMessage" class="div-error-message"></div>

                    <div class="white-block">
                        <div class="col-label block-title">
                            <label class="control-label">Details</label>
                        </div>

                        <div class="form-group form-fields">
                            <div class="col-md-3 col-label">
                                <label class="control-label block-label" for="ddlPreloadedAccountability"> <span class="reqField">* </span>Preloaded Name</label>
                            </div>
                            <div class="col-md-4">
                                <select id="ddlPreloadedAccountability" class="form-control required-field" title="Preloaded Name">
                                    <option value="">- Select an item -</option>
                                </select>
                            </div>
                        </div>

                    </div>

                    <div class="white-block">
                        <div class="col-label block-title">
                            <label class="control-label">Accountability Items</label>
                        </div>
                        <div class="ViewList">
                            <table id="tblPreloadedAccountabilityList"></table>
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>
<div class="modal-footer">
</div>
